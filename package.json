{
  "name": "train-your-dragon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:server": "babel ./src/ --out-dir build/ --copy-files --ignore *.test.js",
    "build:client": "webpack -p --colors --progress",
    "copy:assets": "mkdir -p ./build/public && cp -r ./assets/* ./build/public/",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "clean": "rm -rf ./build",
    "watch": "npm run clean && npm run copy:assets && npm run watch:client & npm run watch:server",
    "watch:client": "webpack -d --watch --colors --progress",
    "watch:server": "NODE_ENV=development nodemon --trace-warnings ./src/server/index.js"
  },
  "dependencies": {
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@magenta/music": "^1.7.0",
    "body-parser": "1.18.3",
