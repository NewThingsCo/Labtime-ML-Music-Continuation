    "compression": "1.7.3",
    "config": "2.0.1",
    "cookie-parser": "1.4.3",
    "express": "^4.16.3",
    "express-winston": "3.0.0",
    "moment": "^2.22.2",
    "pg-monitor": "^1.0.0",
    "pg-promise": "^8.4.6",
    "pm2": "3.0.4",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-linkify": "0.2.2",
    "react-native-modal-datetime-picker": "^6.0.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.8",
    "react-scripts": "1.1.5",
    "react-select": "^2.0.0",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.5",
    "redux-observable": "0.19.0",
    "redux-ui": "0.1.1",
    "rxjs": "5.5.6",
    "serve-favicon": "2.5.0",
    "styled-components": "^3.4.9",
    "winston": "3.1.0"
